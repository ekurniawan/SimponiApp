<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SimponiApp.Views.TracerStudyPage" Title="Tracer Study">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout>
                <ListView x:Name="lvTracerStudy">
                    <ListView.ItemTemplate>
                        <DataTemplate>
                            <TextCell Text="{Binding PERTANYAAN}" />
                        </DataTemplate>
                    </ListView.ItemTemplate>
                </ListView>
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>